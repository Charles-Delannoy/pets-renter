<%= simple_form_for [@pet] do |f| %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :birthdate,  as: :date, start_year: Date.today.year - 20, end_year: Date.today.year %>
<%= f.input :pet_type %>
<%= f.input :race %>
<%= f.input :photo, as: :file %>
<%= f.submit class: "btn btn-primary" %>
<% end %>
